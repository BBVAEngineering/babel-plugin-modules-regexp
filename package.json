{
  "name": "babel-plugin-modules-regexp",
  "version": "0.1.0",
  "description": "Babel plugin to apply a regexp on module names",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BBVAEngineering/babel-plugin-modules-regexp.git"
  },
  "scripts": {
    "start": "qunit --watch tests/**/*-test.js",
    "test": "COVERAGE=true ember test",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 100 --functions 100 --branches 100 --lines 100",
    "commit": "git-cz",
    "lint:js": "eslint --max-warnings 0 ./*.js addon app config tests",
    "lint:hbs": "ember-template-lint .",
    "posttest": "npm run report-coverage && npm run check-coverage && npm run validate-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release",
    "validate-coverage": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0"
  },
  "author": "BBVAEngineering",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-modules-amd": "^7.2.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@ember/optional-features": "^0.7.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-template-lint": "^1.0.0-beta.2",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^0.2.23",
    "eslint": "^5.16.0",
    "eslint-config-bbva": "^2.0.4",
    "eslint-plugin-ember": "^6.3.0",
    "eslint-plugin-node": "^8.0.1",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "json": "^9.0.6",
    "qunit": "^2.9.2",
    "semantic-release": "^15.13.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
